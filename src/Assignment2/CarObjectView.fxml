<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="700.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Assignment2.CarObjectViewController">
    <stylesheets>
        <URL value="@carobjectview.css" />
    </stylesheets>
   <children>
      <Label fx:id="carListLabel" layoutX="14.0" layoutY="14.0" text=" Cars List">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="addNewCarButton" layoutX="523.0" layoutY="416.0" mnemonicParsing="false" onAction="#addANewCarButtonPushed" text="Add a New Car" />
      <TableView fx:id="tableView" layoutX="16.0" layoutY="63.0" prefHeight="232.0" prefWidth="539.0">
        <columns>
          <TableColumn fx:id="manufacturerColumn" prefWidth="108.0" text="Manufacturer" />
          <TableColumn fx:id="modelColumn" prefWidth="108.0" text="Model" />
            <TableColumn fx:id="manufacturingDateColumn" prefWidth="131.0" text="Manufacturing Date" />
            <TableColumn fx:id="maxSpeedColumn" prefWidth="85.0" text="Max Speed" />
            <TableColumn fx:id="carTypeColumn" prefWidth="108.0" text="Car Type " />
        </columns>
      </TableView>
   </children>
</AnchorPane>
